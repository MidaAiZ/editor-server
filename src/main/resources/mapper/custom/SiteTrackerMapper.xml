<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mida.chromeext.modules.dao.SiteTrackerDAO">
    <select id="selectByCountyCode" resultType="com.mida.chromeext.modules.pojo.SiteTracker">
        SELECT *
        FROM site_trackers
        WHERE site_trackers.country_code = #{code} LIMIT 1
    </select>
</mapper>